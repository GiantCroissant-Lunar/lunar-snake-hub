{
  "$schema": "./schemas/config.schema.json",
  "Organization": "YOUR_GITHUB_ORG",
  "Repository": "YOUR_REPO_NAME",
  "Description": "Configuration for lunar-snake-hub Mac Mini infrastructure",
  "MacMini": {
    "TailscaleName": "your-mac-mini-hostname",
    "TailscaleIP": "100.x.x.x",
    "RepoPath": "~/repos/your-repo-name"
  },
  "GLM": {
    "BaseURL": "https://api.z.ai/api/coding/paas/v4",
    "Model": "glm-4.6",
    "EmbeddingModel": "glm-embeddings"
  },
  "Services": {
    "Letta": {
      "Port": 5055,
      "DatabaseURL": "sqlite:///data/letta.db"
    },
    "Qdrant": {
      "Port": 6333,
      "GrpcPort": 6334
    },
    "Gateway": {
      "Port": 5057
    },
    "N8n": {
      "Port": 5678,
      "User": "admin"
    }
  },
  "GitHub": {
    "Enabled": false
  }
}
